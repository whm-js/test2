/*入科、出科管理的时间控件样式start*/
.query-row-date{
	height:50px;padding-left: 30px;margin-top: 16px;margin-bottom: 18px;
	border-bottom: 1px solid #ddd;
}
.query-row-date-title{
	float:left;width:120px;padding:0px;
}
.query-title-label{
	width:120px;margin-top: 5px;text-align:right;
}
.query-row-date-input{
	float:left;width:130px;padding: 0px 5px 0px 0px;
}
.query-row-date-input select{
	float:left;
	width:80px !important;
	height: 34px;
    padding: 6px 12px;
    line-height: 1.42857143;
    color: #555;
    background-color: #fff;
    background-image: none;
    border: 1px solid #ccc;
    border-radius: 4px;
    box-shadow: inset 0 1px 1px rgba(0,0,0,.075);
    box-shadow: none!important;
    outline: 0!important;
    -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075);
    -webkit-transition: border-color ease-in-out .15s,-webkit-box-shadow ease-in-out .15s;
    -o-transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
    transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
}
.query-row-date-input select:focus {
  border-color: #66afe9;
  outline: 0;
  -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(102,175,233,.6);
  box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(102,175,233,.6);
}
.query-row-date-input label{
	display: inline-block;max-width: 100%;margin-top: 5px;margin-left:5px;margin-bottom: 5px;padding-right: 10px;text-align:right;font-weight: 400;
}
/*入科、出科管理的时间控件样式end*/

.query-row{
	clear:left;
	width:100%;height:auto;padding-left: 42px;margin-top:10px;
  overflow: hidden;
}
.query-col{
	float: left;position: relative;width:265px;padding-right: 15px;padding-left: 15px;margin-bottom: 15px;
}
.query-label-control{
	float: left;
	display: inline-block;
	max-width: 100%;padding-right: 10px;margin-top: 5px;margin-bottom: 5px;
	text-align:right;font-weight: 400;
}
.query-input-div{
	float:left;width:155px;padding: 0px;
}
.query-toolbar{
	width: 100%;height: 50px;
	padding:16px 0px 0px 32px;border-top: 1px solid #ddd;
}
